{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/shorturl",
            "dest": "/shorturl.js"
        },
        {
            "src": "/auth",
            "dest": "/auth.js"
        }
    ]
}